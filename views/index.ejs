<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
</head>
<body>
  <h1><%= title %></h1>
  <a href="/new">Add a new message</a>
  <ul>
    <% messages.forEach(function(msg, index) { %>
      <li>
        <strong><%= msg.user %>:</strong> <%= msg.text %> 
        (<%= msg.added.toLocaleString() %>) 
        <a href="/message/<%= index %>">[open]</a>
      </li>
    <% }) %>
  </ul>
</body>
</html>
